<feat xlink:href="#line_156" value="08m--"/><!--  am las lawras li fai talier -->
<feat xlink:href="#line_157" value="08m--"/><!--  hanc la lingua quae aut in quev -->
<feat xlink:href="#line_158" value="08m--"/><!--  cum si l aut toth vituperet -->
<feat xlink:href="#line_159" value="08m--"/><!--  dist ewruins qui tan fud miels -->
<feat xlink:href="#line_160" value="08m--"/><!--  hor a pordud dom deu parlier -->
