<feat xlink:href="#line_107" value="08m--"/><!--  cio li preia paias ab lui -->
<feat xlink:href="#line_108" value="08m--"/><!--  et ewruins fist fincta pais -->
<feat xlink:href="#line_109" value="08m--"/><!--  cio l demonstrat que si paias -->
<feat xlink:href="#line_110" value="08m--"/><!--  quandius in ciel monstier instud -->
<feat xlink:href="#line_111" value="08m--"/><!--  cio l demonstrat amix li fust -->
<feat xlink:href="#line_112" value="08m--"/><!--  mais en avant vos cio aurez -->
<feat xlink:href="#line_113" value="08m--"/><!--  cum ill edrat por mala fid -->
<feat xlink:href="#line_114" value="08m--"/><!--  rex chielperings il se fud morz -->
<feat xlink:href="#line_115" value="08m--"/><!--  por lo regnet lo sowrent toit -->
<feat xlink:href="#line_116" value="08m--"/><!--  vindrent parent e lor amic -->
<feat xlink:href="#line_117" value="08m--"/><!--  li sanct lethgier li ewrui -->
<feat xlink:href="#line_118" value="08m--"/><!--  cio confortent ad ambes duos -->
<feat xlink:href="#line_119" value="08m--"/><!--  que s ent ralgent in lor honors -->
<feat xlink:href="#line_120" value="08m--"/><!--  et sancz lethgiers den fistdra bien -->
<feat xlink:href="#line_121" value="08m--"/><!--  quae s en ralat en s evesquet -->
<feat xlink:href="#line_122" value="08m--"/><!--  et ewruins den fisdra miel -->
<feat xlink:href="#line_123" value="08m--"/><!--  quae donc deveng anatemaz -->
<feat xlink:href="#line_124" value="08m--"/><!--  son quev que il a coronat -->
<feat xlink:href="#line_125" value="08m--"/><!--  toth lo laisera recimer -->
<feat xlink:href="#line_126" value="08m--"/><!--  dominedeu il cio laissat -->
<feat xlink:href="#line_127" value="08m--"/><!--  et a diable comandat -->
<feat xlink:href="#line_128" value="08m--"/><!--  quar donc fud miels et a lui vint -->
<feat xlink:href="#line_129" value="08m--"/><!--  il voluntiers semper reciut -->
<feat xlink:href="#line_130" value="08m--"/><!--  cum fulc en aut grand adunat -->
<feat xlink:href="#line_131" value="08m--"/><!--  lo regne prest a devastar -->
<feat xlink:href="#line_132" value="08m--"/><!--  a foc a flamma vai ardant -->
<feat xlink:href="#line_133" value="08m--"/><!--  et a gladies percutan -->
<feat xlink:href="#line_134" value="08m--"/><!--  por quant il pot tan fai de miel -->
<feat xlink:href="#line_135" value="08m--"/><!--  por deu ne l volt il observer -->
<feat xlink:href="#line_136" value="08m--"/><!--  ciel ne fud nez de medre vius -->
<feat xlink:href="#line_137" value="08m--"/><!--  qui tal exercite vidist -->
<feat xlink:href="#line_138" value="08m--"/><!--  ad ostedun a cilla ciu -->
<feat xlink:href="#line_139" value="08m--"/><!--  dom sanct lethgier vai asalir -->
<feat xlink:href="#line_140" value="08m--"/><!--  ne pot intrer en la ciutat -->
<feat xlink:href="#line_141" value="08m--"/><!--  defors l asist fist i gran miel -->
<feat xlink:href="#line_142" value="08m--"/><!--  et sancz lethgiers mul en fud trist -->
<feat xlink:href="#line_143" value="08m--"/><!--  por ciel tiel miel quae defors vid -->
<feat xlink:href="#line_144" value="07---"/><!--  sos clerjes pres revistiz -->
<feat xlink:href="#line_145" value="08m--"/><!--  et ob ses croix fors s en exit -->
<feat xlink:href="#line_146" value="08m--"/><!--  porr o n exit vol li preier -->
<feat xlink:href="#line_147" value="08m--"/><!--  quae tot ciel miel laisses por deu -->
<feat xlink:href="#line_148" value="08m--"/><!--  ciel ewruins qual hora l vid -->
<feat xlink:href="#line_149" value="08m--"/><!--  penre l rovat lier lo fist -->
